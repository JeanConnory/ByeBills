             x:Class="ByeBills.Views.CategoriaPage"
             xmlns:model="clr-namespace:ByeBills.Models"
             xmlns:viewmodel="clr-namespace:ByeBills.ViewModels"
             x:DataType="viewmodel:CategoriaViewModel"
